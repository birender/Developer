*{
	margin:0px;
	padding:0px;
}
body{
	font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
	margin:0px;
	padding:0px;
	font-size:13px;	
	color:#666;
}
h1,h2,h3,h4,h5,h6, p{ 
	display:block; 
	margin:0px; 
	padding:0px; 
	font-weight:normal;
}
.d_block{
	display:block;
}
input,
textarea{
	outline:none; font-family: "Trebuchet MS", Arial, Helvetica, sans-serif;
}
input[type=submit],
input[type=button],
button{
	cursor:pointer;
}
img{
	border:0px;
}
a{
	text-decoration:none;
	color:#333333;
}
button{
	border:none;
	background:none;
	cursor:pointer;
	color:#f26b02;
}
button:hover{
	color:#DC4D28;
}
.clearAll{
	clear:both;
}
/*div{
	width:100%;
}*/
p{
	margin:0px;
	padding:0px;
}	
ul{
	margin:0px; 
	padding:0px;
	list-style:none;
}
ul li{
	margin:0px;
	padding:0px;
}
.FL{
	float:left;
}
.FR{
	float:right;
}
.cAlign{
	text-align:center;
}
.rAlign{
	text-align:right;
}
.lAlign{
	text-align:left;
}

.width_100{width:100%;}
.font_13{font-size:13px;}
.font_14{font-size:14px;}
.font_15{font-size:15px;}
