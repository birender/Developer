}
.acc_col li a {
	height:31px; padding:0; font-size:14px; color:#333333; display:block; float:left;
}
.acc_col li a span{
	height: 14px; padding:6px 10px 11px 10px; float:left;
}
.acc_col li:hover a,
.acc_col li.active a{
	background: url(../images/top_nav_hover.png); background-position:left top; background-repeat:no-repeat; color:#FFF;
}
.acc_col li:hover a span,
.acc_col li.active a span{
	background: url(../images/top_nav_hover.png); background-position: right -58px; background-repeat:no-repeat; display:block;
}
li.mycart_nav_icon span{
	padding:6px 48px 11px 10px!important;
}
.acc_col .cart_icon{
	background:url(../images/cart-nav-icon.png) no-repeat 0 0; width:33px; height:22px; position:absolute; right:13px; top:4px;
}
.acc_col li:hover .cart_icon{
	background:url(../images/cart-nav-icon.png) no-repeat 0 -28px; width:33px; height:22px; position:absolute; right:13px; top:4px;
}
.cart_items{
	float:left; width:14px; font-size:14px; font-size:13px; color:#3098d5; margin:-3px 0 0 12px; text-align:center;
}
li:hover .cart_items{
	float:left; width:14px; font-size:14px; font-size:13px; color:#FFFFFF; margin:-3px 0 0 12px; text-align:center;
}
/*-----Cart Items------*/
.acc_col li.mycart_nav_icon:hover .cart_item_container{
	display:block; width:250px; position:absolute; right:-4px; top:31px; z-index:999999999;
}
.cart_item_container{
	display:none; width:250px; position:absolute; right:-4px; top:31px; z-index:999999999;
}
.cart_item_container_inner{
	width:240px; float:left; background:#FFFFFF; border:3px solid #d5d5d5;
}
.cart_item_container_inner .cart_item_head{
	width:100%; float:left; border-bottom:1px solid #dbdbdb;
}
.cart_item_container_inner .cart_item_price{
	width:80px; float:left; text-align:left; font-size:16px; color:#333333; padding:5px 0 5px 10px;
}
.cart_item_container_inner .cart_item_quantity{
	width:100px; float:right; text-align:right; font-size:16px; color:#333333; padding:5px 10px 5px 0;
}
.cart_item_container_inner .cart_item_display{
	width:100%; float:left; 
}
.cart_item_container_inner .cart_item_display_row{
	padding:10px; border-bottom:1px solid #dbdbdb; 
}
.cart_item_container_inner .cart_item_display_row:hover{
	background:#f7f7f7;
}
.cart_item_container_inner .cart_product_image{
	width:54px; height:54px; float:left; margin:0 10px 0 0; border:2px solid #d5d5d5;
}
.cart_item_container_inner .cart_product_description{
	width:150px; height:54px; float:right; text-align:left; position:relative;
}
.cart_item_container_inner .cart_product_description a.pro_close_btn{
	width:14px; height:14px; display:block; position:absolute; right:-4px; top:0; background:url(../images/close-btn.png) no-repeat 0 0;
}
.cart_item_container_inner .cart_product_description h4{
	font-size:13px; color:#117fbe; line-height:16px; padding:0 0 2px 0;
}
.cart_item_container_inner .cart_product_description h4 a,
.cart_item_container_inner .cart_product_description h4 a:hover{
	font-size:13px; color:#117fbe; line-height:16px; text-decoration:none; padding:0; margin:0; background:none;
}
.cart_item_container_inner .cart_product_description .pro_qty{
	width:53px; float:left; text-align:left; font-size:13px; color:#333333; display:block; padding:0;
}
.cart_item_container_inner .cart_product_description .pro_price{
	width:53px; float:left; text-align:left; font-size:13px; color:#333333; display:block; padding:0;
}
.cart_item_container_inner .cart_item_footer{
	padding:8px;
}
.cart_item_container_inner .cart_item_footer a,
.cart_item_container_inner .cart_item_footer a:hover{
	width:78px; height:27px; line-height:27px; float:left; text-align:center; font-size:13px; color:#333!important; text-transform:uppercase; background:#e1e1e1; padding:0!important; margin:0!important;	
}
